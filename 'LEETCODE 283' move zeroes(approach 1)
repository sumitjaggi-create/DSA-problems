class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        
        vector<int>array;

        
        int count2 = 0;
        int n = nums.size();

        for(int i=0 ; i<n; i++) {
  
            if(nums[i]!=0){
                array.push_back(nums[i]);
                count2++;
            }
        }

        for(int i= count2; i<n; i++){
            array.push_back(0);
        }
        nums = array;
    }
};
