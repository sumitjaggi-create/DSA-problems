class Solution {
public:
    int singleNumber(vector<int>& nums) {
        
        int ans = -1;
         unordered_map<int, int>count;
        for(int i=0; i<nums.size(); i++){
            count[nums[i]]++;
        }

        for( auto p : count){
            if(p.second==1){
            ans = p.first;
            }
        }
        return ans;

    }
};
