class Solution {
public:
    int reversePairs(vector<int>& nums) {
        
        int count =0;
        for(int i=0; i<nums.size(); i++){
            for(int j=i+1; j<nums.size(); j++){

                if(nums[i] > 2 * nums[j])  // this line will create runtime error because interger limit exceeds for large values {
                    count++;
                }
            }
        }
        return count;
    }
};
